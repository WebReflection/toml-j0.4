{
  "name": "@webreflection/toml-j0.4",
  "version": "1.1.2",
  "description": "A parser only compliant with TOML v0.4.0",
  "type": "module",
  "module": "./toml.js",
  "unpkg": "./toml.js",
  "jsdelivr": "./toml.js",
  "browser": "./toml.js",
  "main": "./cjs/toml.js",
  "types": "./toml.d.ts",
  "exports": {
    ".": {
        "types": "./toml.d.ts",
        "import": "./lib/toml.js",
        "default": "./cjs/toml.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "test": "mocha --bail --reporter spec test/*.js",
    "build": "node build.cjs && ascjs ./lib ./cjs && rollup --config rollup.config.js",
    "benchmark": "node benchmark.cjs",
    "prepublish": "node build.cjs"
  },
  "keywords": [
    "toml",
    "parser",
    "ini"
  ],
  "author": "Jak Wings <jakwings@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git@github.com:jakwings/toml-j0.4.git"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/jakwings/toml-j0.4/blob/master/LICENSE"
    }
  ],
  "bugs": {
    "url": "https://github.com/jakwings/toml-j0.4/issues"
  },
  "homepage": "https://github.com/jakwings/toml-j0.4",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-terser": "^0.4.3",
    "ansi": "0.3.0",
    "ascjs": "^6.0.2",
    "benchmark": "~2.1.4",
    "js-yaml": "~3.9.0",
    "mocha": "~3.4.2",
    "pegjs": "0.10.0",
    "rollup": "^3.29.1",
    "should": "~11.2.1",
    "toml": "~2.3.2"
  }
}
